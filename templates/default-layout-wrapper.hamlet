    <link rel="stylesheet" type="text/css" href="//static.learnyou.org/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="//static.learnyou.org/highlight/styles/default.css">
    <link rel="icon" href=@{FaviconR}>

  <body>
    <div .navbar .navbar-inverse .navbar-static-top role=navigation>
      <div .container>
        <div .navbar-header>
          <button type=button .navbar-toggle .collapsed
                  data-toggle="collapse"
                  data-target=".navbar-collapse">
            <span .sr-only>Toggle navigation
            <span .icon-bar>
            <span .icon-bar>
            <span .icon-bar>
          <a .navbar-brand href=@{RootR}>Learn You!
        <div .collapse .navbar-collapse>
          <ul .nav .navbar-nav .navbar-right>
            <li><a href="http://nightly.learnyou.org/lysa/">LYSA PDFs</a>
            <li><a href="https://webchat.freenode.net/?channels=%23lysa">Webchat</a>
            <li .noscript><a href=@{JSPolicyR}>JavaScript Policy</a>
            <li .noscript><a href=@{LinksR}>Other links</a>
            <script>document.write(`^{dropdownHtml}`);

    <div class="container">
      <div id="main" role="main">
        ^{pageBody pc}
    <footer .footer>
      <div .container>
        <div .table-responsive>
          <table .table .table-bordered .table-hover>
            <tr>
              <td .bottom-cell .text-center .text-muted .small>
